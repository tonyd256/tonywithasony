{
  "name": "tonywithasony",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn dev:html\" \"yarn dev:css\"",
    "dev:html": "npx eleventy --serve",
    "dev:css": "chokidar \"**/*.html\" \"**/*.liquid\" \"**/*.css\" \".eleventy.js\" -i \"_site\" -i \"node_modules\" -c \"yarn build:css\" --initial",
    "build": "npm-run-all build:html build:css",
    "build:html": "npx @11ty/eleventy",
    "build:css": "npx tailwind -i _css/main.css -o _site/css/main.css"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@ampproject/eleventy-plugin-amp": "^0.5.7",
    "autoprefixer": "^10.1.0",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^6.0.0",
    "date-fns": "^2.19.0",
    "dotenv": "^8.2.0",
    "markdown-it": "^12.0.4",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-container": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.0.24"
  }
}
